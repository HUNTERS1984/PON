{% extends "AdminBundle:Layouts:layout.html.twig" %}
{% block title %}{{ "contact.index.title"|trans }}{% endblock %}
{% set active_menu = 'contact' %}

{% block breadcurm %}
    <div class="row">
        <div class="col-md-12 col-xs-12">
            <ul class="breadcrumbs_left">
                <li><span class="text_bre">{{ "contact.index.title"|trans }}</span></li>
            </ul>
        </div>
    </div>
{% endblock %}

{% block page_content %}
    <!-- wrapp_contact -->
    <div class="wrapp_contact">
        <div class="row">
            <div class="col-md-3"></div>
            <div class="col-md-6">
                <div class="contact_content">
                    <h2>{{ "contact.index.contact"|trans }}</h2>
                    <form>
                        <div class="form-group">
                            <input type="text" class="form-control" id="about_what" placeholder="{{ "contact.index.about_what"|trans }}">
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" rows="5" placeholder="{{ "contact.index.tell_us"|trans }}"></textarea>
                        </div>
                        <center><button type="submit" class="btn btn-contact">{{ "common.send"|trans }}</button></center>
                    </form>
                </div>
            </div>
            <div class="col-md-3"></div>
        </div>
    </div>
    <!-- /wrapp_contact -->
{% endblock %}